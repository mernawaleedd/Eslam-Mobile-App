{
	"expo": {
		"name": "KKC",
		"slug": "KKC",
		"version": "1.0.0",
		"newArchEnabled": true,
		"orientation": "portrait",
		"icon": "./assets/images/icon.jpg",
		"scheme": "waterCompany",
		"userInterfaceStyle": "light",

		"splash": {
			"image": "./assets/images/icon.jpg",
			"resizeMode": "contain",
			"padding": 20
		},
		"assetBundlePatterns": ["assets/images/*", "!assets/images/icon.jpg"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/icon.jpg"
			},
			"package": "com.kks"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-image-picker",
				{
					"photosPermission": "The app accesses your photos to let you share them with your friends."
				}
			],
			[
				"expo-location",
				{
					"locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			}
		}
	}
}
